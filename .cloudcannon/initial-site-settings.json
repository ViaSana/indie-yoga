{
  "ssg": "jekyll",
  "mode": "hosted",
  "build_configuration": {
    "source": "",
    "config": "",
    "limit_posts": "",
    "incremental": false,
    "drafts": false,
    "unpublished": false,
    "future": false,
    "lsi": false,
    "trace": false,
    "verbose": false,
    "quiet": false,
    "profile": false,
    "preserved_paths": ".jekyll-cache/,.jekyll-metadata/",
    "preserveOutput": false,
    "includeGit": false,
    "use_local_bundle": true,
    "enable_bundle_cache": true,
    "autosync_gemlock": false,
    "environment_variables": [
      {
        "key": "JEKYLL_ENV",
        "value": "production"
      }
    ],
    "jekyll_environment": "production"
  }
}